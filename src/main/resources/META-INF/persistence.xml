<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="DEFAULT_PU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>io.github.siscultural.entities.Accomplishment</class>
    <class>io.github.siscultural.entities.Budget</class>
    <class>io.github.siscultural.entities.CompanyProvider</class>
    <class>io.github.siscultural.entities.Contract</class>
    <class>io.github.siscultural.entities.Entry</class>
    <class>io.github.siscultural.entities.Functionary</class>
    <class>io.github.siscultural.entities.IndividualProvider</class>
    <class>io.github.siscultural.entities.Locality</class>
    <class>io.github.siscultural.entities.PaymentProposal</class>
    <class>io.github.siscultural.entities.Phone</class>
    <class>io.github.siscultural.entities.Presentation</class>
    <class>io.github.siscultural.entities.Program</class>
    <class>io.github.siscultural.entities.Rubric</class>
    <class>io.github.siscultural.entities.RubricAccount</class>
    <class>io.github.siscultural.entities.Provider</class>
    <class>io.github.siscultural.entities.Committe</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/Siscultural"/>
      <property name="javax.persistence.jdbc.password" value=""/>
      <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
      <property name="javax.persistence.jdbc.user" value="postgres"/>
      <property name="javax.persistence.schema-generation.database.action" value="create"/>
    </properties>
  </persistence-unit>
</persistence>
